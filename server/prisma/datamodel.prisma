type User {
  id: ID! @unique
  name: String!
  email: String
  password: String
  isAdmin: Boolean
}

type Order {
  id: ID! @unique
  total: Float!

  name: String!
  email: String!
}

type OrderProduct {
  id: ID! @unique
  createdAt: DateTime!
  updatedAt: DateTime!

  name: String!
  info: String!
  price: Float!
  quantity: Int!
}

type Product {
  id: ID! @unique
  image: String!
  name: String!
  info: String!
  price: Float!
}
